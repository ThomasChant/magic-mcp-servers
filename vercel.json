{
    "buildCommand": "npm run build",
    "outputDirectory": "dist/client",
    "installCommand": "npm install --force",
    "functions": {
        "api/ssr.js": {
            "maxDuration": 30,
            "includeFiles": "dist/**"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/ssr"
        }
    ]
}