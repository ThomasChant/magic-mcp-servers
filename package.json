{
    "name": "mcp-hub-react",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "node server.js",
        "dev:spa": "vite",
        "build": "npm run build:client && npm run build:server && npm run update:html",
        "build:vercel": "npm run build:client && npm run build:server && npm run update:html && npm run sitemap:build && mv dist/client/index.html dist/client/client-fallback.html && cp favicon.ico dist/client/",    
        "build:client": "vite build --outDir dist/client && cp index-ssr.html dist/client/",
        "build:server": "vite build --config vite.config.ssr.ts",
        "update:html": "node scripts/update-html-assets.js",
        "serve": "NODE_ENV=production node server-simple.js",
        "serve-simple": "NODE_ENV=production node server-simple.js",
        "serve-ssr": "NODE_ENV=production node server-ssr.js",
        "serve-ssr-local": "node serve-ssr-local.js",
        "lint": "eslint .",
        "test": "playwright test",
        "test:e2e": "playwright test",
        "test:voting": "playwright test voting",
        "test:auth-voting": "playwright test voting-authenticated",
        "sitemap:generate": "node scripts/generate-complete-sitemap.js",
        "sitemap:deploy": "node scripts/deploy-sitemaps.js",
        "sitemap:build": "npm run sitemap:generate && npm run sitemap:deploy"
    },
    "dependencies": {
        "@clerk/clerk-react": "^5.32.2",
        "@playwright/test": "^1.53.1",
        "@supabase/supabase-js": "^2.50.2",
        "@tanstack/react-query": "^5.80.11",
        "@vercel/node": "^5.3.0",
        "compression": "^1.8.0",
        "express": "^4.21.2",
        "i18next": "^25.2.1",
        "i18next-browser-languagedetector": "^8.2.0",
        "i18next-http-backend": "^3.0.2",
        "lucide-react": "^0.522.0",
        "node-cron": "^4.2.0",
        "node-fetch": "^2.7.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-i18next": "^15.5.3",
        "react-markdown": "^10.1.0",
        "react-particles": "^2.12.2",
        "react-router": "^7.6.3",
        "react-router-dom": "^6.30.1",
        "rehype-raw": "^7.0.0",
        "remark-gfm": "^4.0.1",
        "sirv": "^3.0.1",
        "tsparticles-slim": "^2.12.0",
        "vike": "^0.4.235",
        "zustand": "^5.0.5"
    },
    "optionalDependencies": {
        "@rollup/rollup-darwin-arm64": "^4.30.3",
        "@rollup/rollup-darwin-x64": "^4.30.3",
        "@rollup/rollup-linux-x64-gnu": "^4.30.3"
    },
    "devDependencies": {
        "@eslint/js": "^9.25.0",
        "@testing-library/jest-dom": "^6.4.6",
        "@testing-library/react": "^16.0.0",
        "@types/node": "^24.0.3",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.2",
        "@vitejs/plugin-react": "^4.4.1",
        "autoprefixer": "^10.4.21",
        "claude-flow": "^1.0.72",
        "dotenv": "^17.0.1",
        "eslint": "^9.25.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^16.0.0",
        "jsdom": "^24.1.0",
        "postcss": "^8.5.6",
        "sharp": "^0.34.2",
        "tailwindcss": "^3.4.17",
        "tsx": "^4.20.3",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.30.1",
        "vite": "^6.3.5",
        "vitest": "^2.0.5"
    }
}
